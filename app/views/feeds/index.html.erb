<div class="displaybody" >
<!-- Loop through each feed -->
<% (0..8).each do |f| %>
	<div class="square">
	   <div class="content">
	        <div class="table">
	            <div class="table-cell">
				<center>
					<%= @feeds["urls"][f]["title"]%>
					<br>
					<%= link_to image_tag(@feeds["urls"][f]["img"], 
						{:class => "rs", 
						 :onerror => " this.src='assets/noimage.gif' "}),
						@feeds["urls"][f]["url"], :target => "_blank" %>
				</center>
	            </div>
	        </div>
	    </div>
	</div>
<% end %>
</div>